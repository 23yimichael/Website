{
  "name": "@personal-website/api",
  "version": "1.1.0",
  "author": "michaelhyi",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "mvn package",
    "clean": "rm -rf .turbo && mvn clean",
    "dev": "source ../../.env && mvn spring-boot:run -Dspring-boot.run.arguments=\"--SPRING_DATASOURCE_URL=$API_SPRING_DATASOURCE_URL --SPRING_DATASOURCE_USERNAME=$API_SPRING_DATASOURCE_USERNAME --SPRING_DATASOURCE_PASSWORD=$API_SPRING_DATASOURCE_PASSWORD --AUTH_WHITELISTED_EMAILS=$API_AUTH_WHITELISTED_EMAILS --AWS_ACCESS_KEY=$API_AWS_ACCESS_KEY --AWS_SECRET_KEY=$API_AWS_SECRET_KEY --AWS_REGION=$API_AWS_REGION --AWS_S3_BUCKETS_BLOG=$API_AWS_S3_BUCKETS_BLOG --SECURITY_JWT_SECRET_KEY=$API_SECURITY_JWT_SECRET_KEY --SECURITY_JWT_EXPIRATION=$API_SECURITY_JWT_EXPIRATION --SECURITY_CORS_ALLOWED_ORIGINS=$API_SECURITY_CORS_ALLOWED_ORIGINS --SECURITY_CORS_ALLOWED_METHODS=$API_SECURITY_CORS_ALLOWED_METHODS --SECURITY_CORS_ALLOWED_HEADERS=$API_SECURITY_CORS_ALLOWED_HEADERS --SECURITY_CORS_EXPOSED_HEADERS=$API_SECURITY_CORS_EXPOSED_HEADERS \"",
    "lint": "mvn validate",
    "setup": "mvn install",
    "test": "mvn test"
  }
}
