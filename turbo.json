{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "globalDotEnv": [".env"],
  "globalEnv": ["NODE_ENV"],
  "pipeline": {
    "build": {
      "dotEnv": [".env"],
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "ci": {},
    "clean": {},
    "@personal-website/admin#dev": {
      "env": ["NEXTAUTH_SECRET", "NEXTAUTH_URL", "NEXT_PUBLIC_API_URL"],
      "cache": false,
      "persistent": true
    },
    "@personal-website/api#dev": {
      "cache": false,
      "persistent": true
    },
    "@personal-website/web#dev": {
      "env": ["NEXT_PUBLIC_API_URL"],
      "cache": false,
      "persistent": true
    },
    "format": {},
    "lint": {},
    "prepare": {},
    "setup": {}
  }
}
